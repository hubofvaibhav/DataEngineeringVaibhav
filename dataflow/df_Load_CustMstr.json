{
	"name": "df_Load_CustMstr",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_CUST_MSTR",
						"type": "DatasetReference"
					},
					"name": "SourceCustMstr"
				},
				{
					"dataset": {
						"referenceName": "ds_MasterChild",
						"type": "DatasetReference"
					},
					"name": "sourceMasterChildExport"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "ds_CUST_MSTR_Sink",
						"type": "DatasetReference"
					},
					"name": "SinkCustMstr"
				},
				{
					"dataset": {
						"referenceName": "ds_MasterChild_Sink",
						"type": "DatasetReference"
					},
					"name": "sinkMasterChild"
				}
			],
			"transformations": [
				{
					"name": "deriveDate"
				},
				{
					"name": "deriveDateMC"
				}
			],
			"scriptLines": [
				"source(output(",
				"          CustomerID as string,",
				"          CustomerName as string,",
				"          City as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     rowUrlColumn: 'sourceFileName',",
				"     wildcardPaths:['CUST_MSTR_*.csv']) ~> SourceCustMstr",
				"source(output(",
				"          MasterID as string,",
				"          ChildID as string,",
				"          RelationshipType as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     rowUrlColumn: 'fileName',",
				"     wildcardPaths:['master_child_export-*.csv']) ~> sourceMasterChildExport",
				"SourceCustMstr derive(Date = toDate(substring(sourceFileName, 10, 8), 'yyyyMMdd')) ~> deriveDate",
				"sourceMasterChildExport derive(Date = toDate(substring(replaceRight(fileName, '.csv', ''), length(fileName) - 11, 8), 'yyyyMMdd'),",
				"          Datekey = substring(replaceRight(fileName, '.csv', ''), length(fileName) - 11, 8)) ~> deriveDateMC",
				"deriveDate sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          CustomerID as integer,",
				"          CustomerName as string,",
				"          City as string,",
				"          Date as date",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError',",
				"     mapColumn(",
				"          CustomerID,",
				"          CustomerName,",
				"          City,",
				"          Date",
				"     )) ~> SinkCustMstr",
				"deriveDateMC sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          MasterID as integer,",
				"          ChildID as integer,",
				"          RelationshipType as string,",
				"          Date as date,",
				"          DateKey as integer",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError',",
				"     mapColumn(",
				"          MasterID,",
				"          ChildID,",
				"          RelationshipType,",
				"          Date,",
				"          DateKey = Datekey",
				"     )) ~> sinkMasterChild"
			]
		}
	}
}